<odoo>
    <record id="journal_entry_own_rule" model="ir.rule">
        <field name="name">Journal: own entries only</field>
        <field name="model_id" ref="model_specialist_journal_entry"/>
        <field name="domain_force">
            ['|', ('user_id', '=', user.id), ('user_id', '=', False)]
        </field>
        <field name="groups" eval="[(4, ref('base.group_user'))]"/>
    </record>
</odoo>
