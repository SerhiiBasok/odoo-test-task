<odoo>
    <record id="journal_entry_list" model="ir.ui.view">
        <field name="name">journal.entry.list</field>
        <field name="model">specialist.journal.entry</field>
        <field name="arch" type="xml">
            <list>
                <field name="date"/>
                <field name="partner_id"/>
                <field name="user_id"/>
                <field name="note"/>
            </list>
        </field>
    </record>

    <record id="journal_entry_form" model="ir.ui.view">
        <field name="name">journal.entry.form</field>
        <field name="model">specialist.journal.entry</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="date"/>
                    <field name="user_id"/>
                    <field name="partner_id"/>
                </group>
                <field name="note"/>
            </form>
        </field>
    </record>
</odoo>
